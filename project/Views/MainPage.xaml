<?xml version="1.0" encoding="UTF-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="pospolsl2024.Views.MainPage"
             Title="Main">

    <Grid Padding="10" ColumnDefinitions="*,*">
        <Frame Grid.Column="0" Margin="10" Padding="10" BackgroundColor="White" CornerRadius="10" HasShadow="True">
            <Button Text="Database" Clicked="OnEditDatabaseButtonClicked" />
        </Frame>
        <Frame Grid.Column="1" Margin="10" Padding="10" BackgroundColor="White" CornerRadius="10" HasShadow="True">
            <Button Text="POS" Clicked="OnPOSButtonClicked" />
        </Frame>
    </Grid>
</ContentPage>
